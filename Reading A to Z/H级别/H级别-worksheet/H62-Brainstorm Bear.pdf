  function enableLogin() {
      $j('#loginOverlay').css({ 'height': $j(document).height()+'px' }).toggle();
      $j('#memberlogin-link').toggleClass( "active" );
      $j('#loginFormDiv').toggle();
      var $username = $j('#username');
      ($username.val() ? $j('#password') : $username).focus();
      return false;
  }

  function disableLogin() {
      $j('#loginFormDiv,#loginOverlay').hide();
      $j('#memberlogin-link').removeClass( "active" );
      return false;
  }

</script>

<!-- Order Now button handler -->
<script type="text/javascript">
    var orderNow = {
        coupon : '',

        getMemberCertificateFromCookie : function (customGaq) {
            //call when user presses OrderNow button to see if the member previously logged in on this or another site.
            $j.ajax({
                type: 'GET',
                url: orderNow.lazMemberTargetUrl,
                crossDomain: true,
                xhrFields: {
                    withCredentials: true
                },
                dataType: 'json',
                success: function (responseData, textStatus, jqXHR) {
                    if (responseData.failed) {
                        console.log("member check: " + responseData.failed);
                        window.location.assign(orderNow.getSubscribeNewBaseUrl()+customGaq+orderNow.coupon);
                    } else {
                        console.log('GET member check succeeded');
                        if(responseData.certificate) {
                            //redirect since system does not yet support cross origin posts to accountsweb
                            var location = orderNow.getSubscribeNewBaseUrl() + orderNow.coupon + "&certificate=" + responseData.certificate + customGaq;
                            //console announces location as navigation target
                            window.location.assign(location);
                        } else {
                            console.log('certificate was null');
                            window.location.assign(orderNow.getSubscribeNewBaseUrl()+customGaq+orderNow.coupon);
                        }
                    }
                },
                error: function (responseData, textStatus, errorThrown) {
                    console.log('GET failed.' + textStatus);
                    window.location.assign(orderNow.getSubscribeNewBaseUrl()+customGaq+orderNow.coupon);
                }
            });
            return true;
        },
        buttonClick : function(customGaq) {  //different buttons can set different campaigns
            if(!customGaq) {
                customGaq = ''; //'&something=...'
            }
            orderNow.init();
            orderNow.getMemberCertificateFromCookie(customGaq);
        },
        getSubscribeBaseUrl : function() {
            return orderNow.accountsWebserver + "/accountsweb/sss/orderLogin.do?siteName=" + orderNow.siteName;
        },
        getSubscribeNewBaseUrl : function() {
            return orderNow.accountsWebserver + "/accountsweb/sss/orderLogin.do?siteName=" + orderNow.siteName;
        },
        setSiteName : function(site) {
            orderNow.siteName = site;
        },
        setAccountsWebserver : function(accountsWebserver) {
            orderNow.accountsWebserver = accountsWebserver;
        },
        setLazMemberTargetUrl : function(lazMemberTargetUrl) {
            orderNow.lazMemberTargetUrl = lazMemberTargetUrl;
        },
        init : function() {
            var div = document.getElementById("orderNowInit");
            var initData = JSON.parse(div.textContent);
            orderNow.setSiteName(initData["siteName"]);
            orderNow.setAccountsWebserver(initData["accountsWebserver"]);
            orderNow.setLazMemberTargetUrl(initData["lazUrl"]);
        }
    };
    //continue to use the prior onclick function call
    var orderNowClick = function(customGaq) {
        orderNow.buttonClick(customGaq)
    }
</script>

